{
  "canisters": {
    "message_board": {
      "type": "custom",
      "main": "src/index.ts",
      "candid": "src/index.did",
      "candid_gen": "automatic",
      "build": "npx azle message_board",
      "wasm": ".azle/message_board/message_board.wasm",
      "gzip": true,
      "metadata": [
        {
          "name": "candid:service",
          "path": "src/index.did"
        },
        {
          "name": "cdk:name",
          "content": "azle"
        }
      ],
      "declarations": {
        "output": "test/dfx_generated/message_board",
        "node_compatibility": true
      }
    }
  }
}